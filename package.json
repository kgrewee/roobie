{
    "name": "roobie",
    "version": "0.1.8",
    "description": "Centralized CSS and JavaScript",
    "author": "Kyle Grewe",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/kgrewee/roobie/issues"
    },
    "homepage": "https://github.com/kgrewee/roobie#readme",
    "keywords": [
        "centralized",
        "css-framework",
        "css",
        "js",
        "html"
    ],
    "main": "roobie.min.js",
    "scripts": {
        "build": "npm-run-all build:dev build:prod docs",
        "build:dev": "cat src/**/*.css src/**/**/*.css  > roobie.css && cat src/**/*.js src/**/**/*.js  > roobie.js",
        "build:prod": "minify roobie.css > roobie.min.css && minify roobie.js > roobie.min.js",
        "docs": "npm-run-all docs:js docs:css",
        "docs:js": "jsdoc roobie.js -d docs/js",
        "docs:css": "",
        "prepare": "husky install"
    },
    "devDependencies": {
        "husky": "^7.0.4",
        "jsdoc": "^3.6.10",
        "minify": "^8.0.3",
        "npm-run-all": "^4.1.5"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kgrewee/roobie.git"
    },
    "dependencies": {},
    "files": [
        "README.md",
        "LICENSE.md",
        "roobie.min.css",
        "roobie.min.js",
        "images/",
        "icons/"
    ]
}
